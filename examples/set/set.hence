x
  subtract '1' [from] depth

y
  subtract '2' [from] depth

main
  [set x to] 'foo'
  [set y to] 'bar'

  write-line concatenate 'x = ' get 'x'
  write-line concatenate 'y = ' get 'y'

  set 'x' [to] 'baz'
  set 'y' [to] 'quux'

  write-line concatenate 'x = ' get 'x'
  write-line concatenate 'y = ' get 'y'

  [return] '0'
